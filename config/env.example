# Catalyst Center Configuration
# Copy this file to .env and fill in your values

# Catalyst Center API Configuration
DNAC_HOST=your-catalyst-center-host.com
DNAC_PORT=443
DNAC_USERNAME=your-username
DNAC_PASSWORD=your-password
DNAC_VERIFY_SSL=true

# Optional: API Token (if using token-based auth)
# DNAC_TOKEN=your-api-token

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/catalyst_center.log

# Template Configuration
TEMPLATE_BASE_PATH=./templates
OUTPUT_PATH=./output
